@prefix bldg:  <http://my-bldg#> .
@prefix brick: <https://brickschema.org/schema/Brick#> .
@prefix ref:   <https://brickschema.org/schema/Brick/ref#> .
@prefix unit:  <http://qudt.org/vocab/unit/> .

bldg:building
a                        brick:Building ;
brick:isMeteredBy        bldg:meter ;
ref:hasExternalReference [ a ref:ExternalReference ; ref:hasExternalReference "OB103" ] ;
brick:area               [ brick:value   "500" ;
brick:hasUnit unit:M2 ] ;
brick:coordinates        [ brick:latitude  "33.9614" ;
brick:longitude "-118.3531" ] ;
brick:hasPart            bldg:zone1, bldg:zone2, bldg:zone3 .

bldg:meter
a              brick:Building_Electrical_Meter ;
brick:hasPoint bldg:building_power .

bldg:building_power
a                        brick:Electric_Power_Sensor ;
brick:hasUnit            unit:W ;
ref:hasExternalReference [ a ref:TimeseriesReference ; ref:hasTimeseriesId "1b3e-29dk-8js7-f54v" ] .

bldg:zone1
a                 brick:Zone ;
brick:hasPoint    bldg:temp_zone1 .

bldg:zone2
a                 brick:Zone ;
brick:hasPoint    bldg:temp_zone2 .

bldg:zone3
a                 brick:Zone ;
brick:hasPoint    bldg:temp_zone3 .

bldg:temp_zone1
a                        brick:Air_Temperature_Sensor ;
brick:hasUnit            unit:DEG_C ;
ref:hasExternalReference [ a ref:TimeseriesReference ; ref:hasTimeseriesId "t29s-jk83-kv82-93fs" ] .

bldg:temp_zone2
a                        brick:Air_Temperature_Sensor ;
brick:hasUnit            unit:DEG_C ;
ref:hasExternalReference [ a ref:TimeseriesReference ; ref:hasTimeseriesId "f29g-js92-df73-l923" ] .

bldg:temp_zone3
a                        brick:Air_Temperature_Sensor ;
brick:hasUnit            unit:DEG_C ;
ref:hasExternalReference [ a ref:TimeseriesReference ; ref:hasTimeseriesId "m93d-ljs9-83ks-29dh" ] .

bldg:hvac_system
a                   brick:HVAC_Equipment ;
brick:feeds         bldg:zone1, bldg:zone2, bldg:zone3 ;
brick:hasPoint      bldg:hvac_power .

bldg:hvac_power
a                        brick:Electric_Power_Sensor ;
brick:hasUnit            unit:W ;
ref:hasExternalReference [ a ref:TimeseriesReference ; ref:hasTimeseriesId "29dh-8ks3-fvjs-d92e" ] .